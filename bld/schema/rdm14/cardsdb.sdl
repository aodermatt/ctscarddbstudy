CREATE TABLE card_info
(
    access_rules_record_addr UINT64 DEFAULT 0 NOT NULL,
    card_access_type UINT8 DEFAULT 0 NOT NULL,
    token_digest BINARY(35) UNIQUE KEY  NOT NULL,
    last_digits CHAR(4) DEFAULT "" NOT NULL
);

CREATE TABLE card_access_rules
(
    access_data BINARY(500) DEFAULT x'' NOT NULL
);

